<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title  %> </title>
</head>
<body>
    <h1><%= Judul  %></h1>
    <table class="table table-responsive" border="1">
        <thead>
            <tr>
                <th>No</th>
            <th>Nama</th>
            <th>email</th>
            <th>Kontak</th>
            </tr>
            
        </thead>
        <tbody>
            <% kontak.forEach(data => {%>
            <tr>
               
                    <td><%= data.id %></td>
                    <td><%= data.nama %></td>
                    <td><%= data.email %></td>
                    <td><%= data.hp %></td>
                

            </tr>
            <%}); %>
        </tbody>
    </table>
</body>
</html>